#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/NiaServer-Docs/node_modules/.pnpm/vuepress-vite@2.0.0-beta.64_@vuepress+client@2.0.0-beta.64_vue@3.2.47/node_modules/vuepress-vite/bin/node_modules:/mnt/d/NiaServer-Docs/node_modules/.pnpm/vuepress-vite@2.0.0-beta.64_@vuepress+client@2.0.0-beta.64_vue@3.2.47/node_modules/vuepress-vite/node_modules:/mnt/d/NiaServer-Docs/node_modules/.pnpm/vuepress-vite@2.0.0-beta.64_@vuepress+client@2.0.0-beta.64_vue@3.2.47/node_modules:/mnt/d/NiaServer-Docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/NiaServer-Docs/node_modules/.pnpm/vuepress-vite@2.0.0-beta.64_@vuepress+client@2.0.0-beta.64_vue@3.2.47/node_modules/vuepress-vite/bin/node_modules:/mnt/d/NiaServer-Docs/node_modules/.pnpm/vuepress-vite@2.0.0-beta.64_@vuepress+client@2.0.0-beta.64_vue@3.2.47/node_modules/vuepress-vite/node_modules:/mnt/d/NiaServer-Docs/node_modules/.pnpm/vuepress-vite@2.0.0-beta.64_@vuepress+client@2.0.0-beta.64_vue@3.2.47/node_modules:/mnt/d/NiaServer-Docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress-vite/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../vuepress-vite/bin/vuepress.js" "$@"
fi
